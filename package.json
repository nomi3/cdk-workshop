{
  "name": "cdk-workshop",
  "version": "0.1.0",
  "bin": {
    "cdk-workshop": "bin/cdk-workshop.js"
  },
  "scripts": {
    "build": "cdk synth",
    "list": "cdk list",
    "diff": "cdk diff",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "eslint --fix"
  },
  "eslintConfig": {
    "env": {
      "jest/globals": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "standard-with-typescript"
    ],
    "plugins": [
      "@typescript-eslint",
      "jest"
    ],
    "rules": {
      "no-new": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.16.3",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "aws-cdk": "^1.16.3",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-autoscaling": "^1.18.0",
    "@aws-cdk/aws-ec2": "^1.18.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.18.0",
    "@aws-cdk/aws-iam": "^1.18.0",
    "@aws-cdk/aws-sns": "^1.16.3",
    "@aws-cdk/aws-sns-subscriptions": "^1.16.3",
    "@aws-cdk/aws-sqs": "^1.16.3",
    "@aws-cdk/core": "^1.16.3"
  }
}
