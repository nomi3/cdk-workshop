{
  "name": "cdk-workshop",
  "version": "0.1.0",
  "bin": {
    "cdk-workshop": "bin/cdk-workshop.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "standardx **/*.ts",
    "lint:fix": "standardx --fix"
  },
  "eslintConfig": {
    "env": {
      "jest/globals": true
    },
    "plugins": [
      "jest"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.16.3",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "aws-cdk": "^1.16.3",
    "eslint-plugin-jest": "^23.1.1",
    "jest": "^24.9.0",
    "standardx": "^5.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-autoscaling": "^1.18.0",
    "@aws-cdk/aws-ec2": "^1.18.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.18.0",
    "@aws-cdk/aws-sns": "^1.16.3",
    "@aws-cdk/aws-sns-subscriptions": "^1.16.3",
    "@aws-cdk/aws-sqs": "^1.16.3",
    "@aws-cdk/core": "^1.16.3"
  }
}
